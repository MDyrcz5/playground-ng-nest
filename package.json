{
  "name": "playground-ng-nest",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "run:host": "nx run playground-ng-nest:serve-mfe",
    "lint": "prettier --check .",
    "api:start:prod": "node dist/apps/api/main.js",
    "api:build:prod": "nx build api --prod",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --create-only && npx prisma migrate dev",
    "prisma:apply": "yarn prisma:migrate && yarn prisma:generate",
    "prisma:studio": "npx prisma studio"
  },
  "private": true,
  "dependencies": {
